use Drupal\Core\Render\Markup;


/**
 * Implements hook_preprocess_HOOK()
 */

function porbiota_bs_preprocess_menu(&$variables){
  foreach($variables['items'] as &$link){
    $link['title'] = Markup::create($link['title']);
  }
}
